{
  "name": "knightjdr.github.io",
  "homepage": "https://knightjdr.github.io/",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knightjdr/knightjdr.github.io.git"
  },
  "private": false,
  "dependencies": {
    "ajv": "^6.4.0",
    "isomorphic-fetch": "^2.2.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-transition": "^1.2.1",
    "react-scripts": "3.0.1",
    "react-share": "^2.2.0",
    "react-tos": "^0.1.0",
    "vivus": "^0.4.4"
  },
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "deploy": "gh-pages -b master -d build",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "gh-pages": "^2.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/setupTests.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
